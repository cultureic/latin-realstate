{
  "name": "latin-realstate",
  "version": "0.1.0",
  "description": "a platform for showcasing realstate assets",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "type-check": "turbo type-check",
    "contracts:compile": "turbo run compile --filter=hardhat",
    "contracts:test": "turbo run test --filter=hardhat",
    "contracts:deploy": "turbo run deploy --filter=hardhat",
    "contracts:deploy:alfajores": "turbo run deploy:alfajores --filter=hardhat",
    "contracts:deploy:sepolia": "turbo run deploy:sepolia --filter=hardhat",
    "contracts:deploy:celo": "turbo run deploy:celo --filter=hardhat"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "turbo": "^1.13.4",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@8.10.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@selfxyz/contracts": "^1.2.3",
    "@selfxyz/core": "1.1.0-beta.7",
    "@selfxyz/qrcode": "^1.0.15",
    "ether": "^0.0.9",
    "pnpm": "^10.20.0"
  }
}
